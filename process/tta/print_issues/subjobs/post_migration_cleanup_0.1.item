    <elementParameter field="MEMO_JAVA" name="CODE" value="String dirPath=((String)globalMap.get(&quot;tFileList_1_CURRENT_FILEPATH&quot;));&#xA;String dirName=((String)globalMap.get(&quot;tFileList_1_CURRENT_FILE&quot;));&#xA;final String monthArray[]={&quot;jan&quot;,&quot;feb&quot;,&quot;mar&quot;,&quot;apr&quot;,&quot;may&quot;,&quot;jun&quot;,&quot;jul&quot;,&quot;aug&quot;,&quot;sep&quot;,&quot;oct&quot;,&quot;nov&quot;,&quot;dec&quot;};&#xA;String srcTemplate=context.defaultPageTemplate;&#xA;String title=dirName;&#xA;String year=&quot;&quot;;&#xA;boolean isMonthPage=false;&#xA;for(String month:monthArray){&#xA;&#x9;if(dirName.toLowerCase().equalsIgnoreCase(month)){&#xA;&#x9;&#x9;srcTemplate=context.landingPageTemplate;&#xA;&#x9;&#x9;String temp = dirPath.substring(0, dirPath.lastIndexOf(&quot;/&quot;));&#xA;&#x9;&#x9;year = temp.substring(temp.lastIndexOf(&quot;/&quot;) + 1);&#xA;  &#x9;&#x9;title=Utility.getIssuePageTitle(year, dirName);&#xA;&#x9;&#x9;isMonthPage=true;&#xA;&#x9;&#x9;break;&#xA;&#x9;}&#xA;}&#xA;//System.out.println(&quot;dirName&quot;+dirName);&#xA;Utility.createContentXML(dirPath,srcTemplate,&quot;jcr:title&quot;,title);&#xA;if(isMonthPage){&#xA;Utility.updateXmlFile(dirPath+&quot;/.content.xml&quot;, &quot;jcr:content&quot;, &quot;issuedate&quot;,Utility.getIssueDate(year, dirName));&#xA;Utility.updateXmlFile(dirPath+&quot;/.content.xml&quot;, &quot;image&quot;, &quot;fileReference&quot;,Utility.getIssueCover(dirPath,(org.json.JSONObject)context.issueCoversMap));&#xA;}"/>
    <elementParameter field="MEMO_IMPORT" name="IMPORT" value="import java.io.File;&#xA;&#xA;import org.apache.commons.io.FileUtils;&#xA;"/>
